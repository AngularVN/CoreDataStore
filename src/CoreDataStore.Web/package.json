{
  "name": "CoreDataStore",
  "version": "1.0.0",
  "description": "CoreDataStore Angular2 with TypeScript and Gulp",
  "private": true,
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "compile": "./node_modules/.bin/gulp compile",
    "build": "./node_modules/.bin/gulp build",
    "postinstall": "./node_modules/.bin/typings install;./node_modules/.bin/gulp build",
    "start": "concurrently --kill-others \"./node_modules/.bin/gulp watch\" \"./node_modules/.bin/lite-server\"",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "typings": "./node_modules/.bin/typings"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kolorobot/angular2-typescript-gulp.git"
  },
  "author": "Rafa≈Ç Borowiec",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kolorobot/angular2-typescript-gulp/issues"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "moment": "2.13.0",
    "ng2-bootstrap": "1.0.16",
    "ng2-pagination": "^0.3.4",
    "ng2-select": "^1.0.3",
    "systemjs": "0.19.27",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-less": "^3.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.12.0",
    "lite-server": "^2.2.0",
    "tslint": "^3.5.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
