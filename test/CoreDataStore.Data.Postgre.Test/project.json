{
  "version": "1.0.0-*",

  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "CoreDataStore.Data": "1.0.0-*",
    "CoreDataStore.Domain": "1.0.0-*",
    "dotnet-test-xunit": "1.0.0-rc2-*",
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview1-final",
    "Npgsql.EntityFrameworkCore.PostgreSQL": "1.0.0-rc2-release1",
    "Microsoft.NETCore.App": {
      "version": "1.0.0-rc2-3002702",
      "type": "platform"
    },
    "xunit": "2.1.0-rc2-*",
    "CoreDataStore.Data.Postgre": "1.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
